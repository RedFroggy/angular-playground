{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run swagger && npm run serve",
    "serve": "ng serve --watch --o --proxy-config ./webpack/proxy-dev.conf.js --configuration=dev --ssl --ssl-cert ./webpack/certificates/webpack.localhost.certificate.crt  --ssl-key ./webpack/certificates/webpack.localhost.private.key",
    "build": "npm run swagger && ng build --prod --configuration=production",
    "lint": "npm run lint:ts && npm run lint:file",
    "lint:ts": "ng lint",
    "lint:file": "node_modules/@ls-lint/ls-lint/bin/cli.js",
    "e2e:ci": "npm run swagger && ./node_modules/.bin/cypress run -e TAGS='not @ignore'",
    "e2e:ci:concurrent": "npm run swagger && concurrently --success first --kill-others --no-color --raw \"ng serve --configuration=dev\" \"wait-on http-get://localhost:9000 && ./node_modules/.bin/cypress run -e TAGS='not @ignore'\"",
    "e2e:open": "npm run swagger && ./node_modules/.bin/cypress open",
    "e2e:open:concurrent": "npm run swagger && concurrently \"ng serve --configuration=dev\" \"./node_modules/.bin/cypress open\"",
    "test": "npm run swagger && jest --logHeapUsage -w=6 --testPathIgnorePatterns .spec.js --config ./src/test/jest.config.js",
    "test:ci": "npm run test -- --coverage --ci",
    "test:watch": "npm run test -- --watch",
    "release": "semantic-release",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "swagger": "node_modules/.bin/ng-swagger-gen --config swagger-config.json",
    "aws-cdk": "cdk",
    "aws-stack-build": "tsc --project deploy/tsconfig.json",
    "aws-synth": "npm run aws-stack-build && npm run aws-cdk synth",
    "aws-deploy": "npm run aws-stack-build && npm run aws-cdk bootstrap && npm run aws-cdk deploy -- --require-approval=never"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.9",
    "@angular/common": "11.2.9",
    "@angular/compiler": "11.2.9",
    "@angular/core": "11.2.9",
    "@angular/forms": "11.2.9",
    "@angular/localize": "11.2.9",
    "@angular/platform-browser": "11.2.9",
    "@angular/platform-browser-dynamic": "11.2.9",
    "@angular/router": "11.2.9",
    "@fortawesome/angular-fontawesome": "0.8.1",
    "@fortawesome/fontawesome-free": "5.15.2",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@ng-bootstrap/ng-bootstrap": "9.1.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "class-transformer": "0.4.0",
    "lodash": "4.17.21",
    "messageformat": "2.3.0",
    "ng-jhipster": "0.16.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.1",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "0.1100.7",
    "@angular/cli": "11.0.7",
    "@angular/compiler-cli": "11.2.9",
    "@angular/language-service": "11.2.9",
    "@aws-cdk/aws-certificatemanager": "1.85.0",
    "@aws-cdk/aws-cloudfront": "1.85.0",
    "@aws-cdk/aws-iam": "1.85.0",
    "@aws-cdk/aws-s3": "1.85.0",
    "@aws-cdk/aws-s3-deployment": "1.85.0",
    "@aws-cdk/core": "1.85.0",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@ls-lint/ls-lint": "1.9.2",
    "@ngneat/spectator": "7.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/gitlab": "6.0.9",
    "@types/jasmine": "3.6.9",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "8.10.66",
    "aws-cdk": "1.85.0",
    "codelyzer": "6.0.1",
    "concurrently": "6.0.1",
    "copy-webpack-plugin": "5.1.2",
    "cypress": "7.2.0",
    "deep-keys": "0.5.0",
    "husky": "6.0.0",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "jest": "26.6.3",
    "jest-html-reporter": "3.3.0",
    "jest-preset-angular": "8.4.0",
    "merge-jsons-webpack-plugin": "1.0.21",
    "moment-locales-webpack-plugin": "1.2.0",
    "ng-mocks": "11.10.0",
    "ng-swagger-gen": "2.3.0",
    "ngx-translate-messageformat-compiler": "4.9.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "renovate": "24.22.2",
    "semantic-release": "17.4.2",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "tslint-angular": "3.0.3",
    "typescript": "4.2.4",
    "wait-on": "5.3.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-merge": "5.7.3"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  }
}
