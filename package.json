{
  "name": "angular-cli",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve --watch --o --proxy-config ./webpack/proxy-dev.conf.js --configuration=dev --ssl --ssl-cert ./webpack/certificates/webpack.localhost.certificate.crt  --ssl-key ./webpack/certificates/webpack.localhost.private.key",
    "build": "ng build --prod --configuration=production",
    "lint": "ng lint",
    "e2e:ci": "./node_modules/.bin/cypress run -e TAGS='not @ignore'",
    "e2e:ci:concurrent": "concurrently --success first --kill-others --no-color --raw \"ng serve --configuration=dev\" \"wait-on http-get://localhost:9000 && ./node_modules/.bin/cypress run -e TAGS='not @ignore'\"",
    "e2e:open": "./node_modules/.bin/cypress open",
    "e2e:open:concurrent": "concurrently \"ng serve --configuration=dev\" \"./node_modules/.bin/cypress open\"",
    "test": "jest --logHeapUsage -w=6 --testPathIgnorePatterns .spec.js --config ./src/test/jest.config.js",
    "test:ci": "npm run test -- --coverage --ci",
    "test:watch": "npm run test -- --watch",
    "release": "semantic-release",
    "update-dependencies": "renovate",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.0",
    "@angular/common": "8.1.0",
    "@angular/compiler": "8.1.0",
    "@angular/core": "8.1.0",
    "@angular/forms": "8.1.0",
    "@angular/platform-browser": "8.1.0",
    "@angular/platform-browser-dynamic": "8.1.0",
    "@angular/router": "8.1.0",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@ng-bootstrap/ng-bootstrap": "5.1.4",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@sentry/browser": "^5.9.1",
    "messageformat": "2.3.0",
    "ng-jhipster": "0.11.4",
    "ngx-translate-cache": "0.1.1",
    "ngx-translate-messageformat-compiler": "4.5.0",
    "rxjs": "6.4.0",
    "tslib": "1.9.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "8.2.0",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "0.801.0",
    "@angular/cli": "8.1.0",
    "@angular/compiler-cli": "8.1.0",
    "@angular/language-service": "8.1.0",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@ngneat/spectator": "4.5.2",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/gitlab": "4.0.4",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "5.0.0",
    "concurrently": "5.0.0",
    "copy-webpack-plugin": "5.0.5",
    "cypress": "3.6.1",
    "husky": "3.0.9",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "merge-jsons-webpack-plugin": "1.0.20",
    "moment-locales-webpack-plugin": "1.1.0",
    "ng-mocks": "8.1.0",
    "renovate": "19.66.13",
    "semantic-release": "15.13.30",
    "ts-node": "7.0.0",
    "tslint": "5.15.0",
    "typescript": "3.4.3",
    "wait-on": "3.3.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-merge": "4.2.2"
  }
}
