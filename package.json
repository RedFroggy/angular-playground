{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run swagger && npm run serve",
    "serve": "ng serve --watch --o --proxy-config ./webpack/proxy-dev.conf.js --configuration=dev --ssl --ssl-cert ./webpack/certificates/webpack.localhost.certificate.crt  --ssl-key ./webpack/certificates/webpack.localhost.private.key",
    "build": "npm run swagger && ng build --prod --configuration=production",
    "lint": "ng lint",
    "e2e:ci": "npm run swagger && ./node_modules/.bin/cypress run -e TAGS='not @ignore'",
    "e2e:ci:concurrent": "npm run swagger && concurrently --success first --kill-others --no-color --raw \"ng serve --configuration=dev\" \"wait-on http-get://localhost:9000 && ./node_modules/.bin/cypress run -e TAGS='not @ignore'\"",
    "e2e:open": "npm run swagger && ./node_modules/.bin/cypress open",
    "e2e:open:concurrent": "npm run swagger && concurrently \"ng serve --configuration=dev\" \"./node_modules/.bin/cypress open\"",
    "test": "npm run swagger && jest --logHeapUsage -w=6 --testPathIgnorePatterns .spec.js --config ./src/test/jest.config.js",
    "test:ci": "npm run test -- --coverage --ci",
    "test:watch": "npm run test -- --watch",
    "release": "semantic-release",
    "update-dependencies": "renovate",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "swagger": "node_modules/.bin/ng-swagger-gen --config swagger-config.json",
    "aws-cdk": "cdk",
    "aws-stack-build": "tsc --project deploy/tsconfig.json",
    "aws-synth": "npm run aws-stack-build && npm run aws-cdk synth",
    "aws-deploy": "npm run aws-stack-build && npm run aws-cdk bootstrap && npm run aws-cdk deploy -- --require-approval=never"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.2",
    "@angular/common": "9.1.2",
    "@angular/compiler": "9.1.2",
    "@angular/core": "9.1.2",
    "@angular/forms": "9.1.2",
    "@angular/localize": "9.1.2",
    "@angular/platform-browser": "9.1.2",
    "@angular/platform-browser-dynamic": "9.1.2",
    "@angular/router": "9.1.2",
    "@fortawesome/angular-fontawesome": "0.6.1",
    "@fortawesome/fontawesome-free": "5.13.0",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@ng-bootstrap/ng-bootstrap": "6.1.0",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "class-transformer": "0.2.3",
    "messageformat": "2.3.0",
    "ng-jhipster": "0.12.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "tslib": "1.14.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "0.1100.2",
    "@angular/cli": "9.1.12",
    "@angular/compiler-cli": "9.1.2",
    "@angular/language-service": "9.1.2",
    "@aws-cdk/aws-certificatemanager": "1.38.0",
    "@aws-cdk/aws-cloudfront": "1.38.0",
    "@aws-cdk/aws-iam": "1.38.0",
    "@aws-cdk/aws-s3": "1.38.0",
    "@aws-cdk/aws-s3-deployment": "1.38.0",
    "@aws-cdk/core": "1.38.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@ngneat/spectator": "5.9.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/gitlab": "6.0.4",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "8.10.60",
    "aws-cdk": "1.38.0",
    "codelyzer": "5.2.2",
    "concurrently": "5.2.0",
    "copy-webpack-plugin": "5.1.1",
    "cypress": "4.12.1",
    "husky": "4.2.5",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "jest": "25.5.4",
    "jest-preset-angular": "8.1.3",
    "merge-jsons-webpack-plugin": "1.0.21",
    "moment-locales-webpack-plugin": "1.2.0",
    "ng-mocks": "9.6.4",
    "ng-swagger-gen": "2.2.0",
    "ngx-translate-messageformat-compiler": "4.7.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "renovate": "19.216.3",
    "semantic-release": "17.0.7",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "tslint-angular": "3.0.2",
    "typescript": "3.8.3",
    "wait-on": "5.2.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-merge": "4.2.2"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  }
}
