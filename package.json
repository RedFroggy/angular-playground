{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run swagger && npm run serve",
    "serve": "ng serve --watch --o --proxy-config ./webpack/proxy-dev.conf.js --configuration=dev --ssl --ssl-cert ./webpack/certificates/webpack.localhost.certificate.crt  --ssl-key ./webpack/certificates/webpack.localhost.private.key",
    "build": "npm run swagger && ng build --prod --configuration=production",
    "lint": "ng lint",
    "e2e:ci": "npm run swagger && ./node_modules/.bin/cypress run -e TAGS='not @ignore'",
    "e2e:ci:concurrent": "npm run swagger && concurrently --success first --kill-others --no-color --raw \"ng serve --configuration=dev\" \"wait-on http-get://localhost:9000 && ./node_modules/.bin/cypress run -e TAGS='not @ignore'\"",
    "e2e:open": "npm run swagger && ./node_modules/.bin/cypress open",
    "e2e:open:concurrent": "npm run swagger && concurrently \"ng serve --configuration=dev\" \"./node_modules/.bin/cypress open\"",
    "test": "npm run swagger && jest --logHeapUsage -w=6 --testPathIgnorePatterns .spec.js --config ./src/test/jest.config.js",
    "test:ci": "npm run test -- --coverage --ci",
    "test:watch": "npm run test -- --watch",
    "release": "semantic-release",
    "update-dependencies": "renovate",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats.json",
    "swagger": "node_modules/.bin/ng-swagger-gen --config swagger-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/forms": "9.1.0",
    "@angular/localize": "9.1.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@ng-bootstrap/ng-bootstrap": "6.0.2",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "class-transformer": "0.2.3",
    "messageformat": "2.3.0",
    "ng-jhipster": "0.12.0",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@ngneat/spectator": "4.5.2",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/gitlab": "4.0.4",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.10.59",
    "codelyzer": "5.0.0",
    "concurrently": "5.0.0",
    "copy-webpack-plugin": "5.0.5",
    "cypress": "3.6.1",
    "husky": "3.0.9",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "merge-jsons-webpack-plugin": "1.0.20",
    "moment-locales-webpack-plugin": "1.1.0",
    "ng-mocks": "8.1.0",
    "ng-swagger-gen": "2.2.0",
    "ngx-translate-messageformat-compiler": "4.6.0",
    "prettier": "2.0.3",
    "pretty-quick": "2.0.1",
    "reflect-metadata": "0.1.13",
    "renovate": "19.195.5",
    "semantic-release": "17.0.4",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "tslint-angular": "3.0.2",
    "typescript": "3.8.3",
    "wait-on": "3.3.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-merge": "4.2.2"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  }
}
