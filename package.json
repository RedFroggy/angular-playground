{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run openapi:generate && npm run serve",
    "serve": "ng serve --watch --o --proxy-config ./webpack/proxy-dev.conf.js --configuration=dev --ssl --ssl-cert ./webpack/certificates/webpack.localhost.certificate.crt  --ssl-key ./webpack/certificates/webpack.localhost.private.key",
    "build": "npm run openapi:generate && ng build --prod --configuration=production",
    "lint": "npm run lint:ts && npm run lint:file",
    "lint:ts": "ng lint",
    "lint:file": "node_modules/@ls-lint/ls-lint/bin/cli.js",
    "codecov": "codecov",
    "e2e:ci": "npm run openapi:generate && cypress run -e TAGS='not @ignore'",
    "e2e:ci:concurrent": "npm run openapi:generate && concurrently --success first --kill-others --no-color --raw \"ng serve --configuration=dev\" \"wait-on http-get://localhost:9000 && cypress run -e TAGS='not @ignore'\"",
    "e2e:open": "npm run openapi:generate && cypress open",
    "e2e:open:concurrent": "npm run openapi:generate && concurrently \"ng serve --configuration=dev\" \"cypress open\"",
    "test": "npm run openapi:generate && npm run lint && jest --logHeapUsage -w=2 --testPathIgnorePatterns .spec.js --config ./src/test/jest.config.js",
    "test:ci": "npm run test -- --coverage --ci",
    "test:watch": "npm run test -- --watch",
    "release": "semantic-release",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "openapi:generate": "node_modules/.bin/openapi-generator-cli generate -i https://petstore3.swagger.io/api/v3/openapi.json -g typescript-angular -o src/app/api --additional-properties=ngVersion=11.2.11,npmName=restClient,supportsES6=true,npmVersion=6.9.0",
    "aws-cdk": "cdk",
    "aws-stack-build": "tsc --project deploy/tsconfig.json",
    "aws-synth": "npm run aws-stack-build && npm run aws-cdk synth",
    "aws-deploy": "npm run aws-stack-build && npm run aws-cdk bootstrap && npm run aws-cdk deploy -- --require-approval=never",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.14",
    "@angular/common": "11.2.14",
    "@angular/compiler": "11.2.14",
    "@angular/core": "11.2.14",
    "@angular/forms": "11.2.14",
    "@angular/localize": "11.2.14",
    "@angular/platform-browser": "11.2.14",
    "@angular/platform-browser-dynamic": "11.2.14",
    "@angular/router": "11.2.14",
    "@ng-bootstrap/ng-bootstrap": "9.1.3",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "class-transformer": "0.4.0",
    "lodash": "4.17.21",
    "messageformat": "2.3.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.0.0",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.1",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "0.1102.14",
    "@angular-eslint/builder": "4.3.0",
    "@angular-eslint/eslint-plugin": "4.3.0",
    "@angular-eslint/eslint-plugin-template": "4.3.0",
    "@angular-eslint/schematics": "4.3.0",
    "@angular-eslint/template-parser": "4.3.0",
    "@angular/cli": "11.2.14",
    "@angular/compiler-cli": "11.2.14",
    "@angular/language-service": "11.2.14",
    "@aws-cdk/aws-certificatemanager": "1.101.0",
    "@aws-cdk/aws-cloudfront": "1.101.0",
    "@aws-cdk/aws-iam": "1.101.0",
    "@aws-cdk/aws-s3": "1.101.0",
    "@aws-cdk/aws-s3-deployment": "1.101.0",
    "@aws-cdk/core": "1.101.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@ls-lint/ls-lint": "1.9.2",
    "@ngneat/spectator": "7.1.0",
    "@openapitools/openapi-generator-cli": "2.2.6",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/github": "7.2.3",
    "@types/jasmine": "3.6.9",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "8.10.66",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "aws-cdk": "1.101.0",
    "codecov": "3.8.3",
    "concurrently": "6.0.1",
    "copy-webpack-plugin": "5.1.2",
    "cypress": "7.2.0",
    "deep-keys": "0.5.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "6.0.0",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "26.6.3",
    "jest-html-reporter": "3.3.0",
    "jest-preset-angular": "8.4.0",
    "merge-jsons-webpack-plugin": "1.0.21",
    "moment-locales-webpack-plugin": "1.2.0",
    "ng-mocks": "11.10.0",
    "ngx-translate-messageformat-compiler": "4.9.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.1",
    "renovate": "25.5.0",
    "semantic-release": "17.4.7",
    "ts-node": "9.1.1",
    "typescript": "4.1.5",
    "wait-on": "5.3.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-merge": "5.7.3"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  }
}
