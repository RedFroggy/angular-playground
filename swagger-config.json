{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "https://petstore.swagger.io/v2/swagger.json",
  "output": "src/app/api",
  "prefix": "Api",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "desc",
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false,
  "camelCase": false,
  "customFileSuffix": {
    "model": "",
    "service": ".service"
  },
  "templates": "swagger",
  "timeout": 20000
}
